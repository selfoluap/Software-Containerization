sudo docker build backend/server/.
sudo docker images
sudo docker tag <podid> server:v1
sudo docker run -p 5000:5000 server:v1
http://localhost:5000/server/v1.0/
sudo docker push localhost:32000/


curl -X POST http://localhost:5000/server/create_todo -H 'Content-Type: application/json' -d '{"title":"my title 3","description":"my desc 3"}'
curl -X DELETE http://localhost:5000/server/delete_todo/3

